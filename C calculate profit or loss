#include<stdio.h>
#include<conio.H>
void main()
{
    float cp,sp,p,l;
    printf("enter cost price");
    scanf("%f",&cp);
    printf("enter sellinh price");
    scanf("%f",&sp);
    p=sp-cp;
    l=cp-sp;
    if (p>0)
    printf("the seller made a profit rs.%f\n",p);
    if (l>0)
    printf("the seller incurres loss of rs.%f/n",l);
    if (p==0)
printf("there is no loss , no profit\n");
    
}
